{
  "name": "JPlugCheck",
  "description": "Jalios Plugins Version Checker – Identifie facilement les plugins obsolète depuis la page de gestion des modules Jalios.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["scripting", "tabs"],
  "host_permissions": [
    "<all_urls>",
    "https://community.jalios.com/*"
  ],
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_title": "Identifiez vos plugins Jalios obsolète",
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}